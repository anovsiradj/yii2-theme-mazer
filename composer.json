{
    "name": "anovsiradj/yii2-theme-mazer",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "yii2\\theme\\mazer\\": "./"
        }
    },
    "require": {
        "php": ">=7",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap5": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}